{
    "name": "Autofill Forms",
    "manifest_version": 3,
    "version": "1.0",
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": ["<all_urls>"]
        }
    ]
}